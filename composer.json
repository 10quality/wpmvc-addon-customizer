{
    "name": "10quality/wpmvc-addon-customizer",
    "description": "Easy custom WordPress customizer settings for WordPress MVC (WPMVC) framework.",
    "license": "MIT",
    "keywords": ["wpmvc","wordpress","customizer"],
    "require": {
        "php": ">=5.4.0",
        "10quality/wpmvc-core": ">=3.1.0",
        "10quality/wpmvc-addon": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "10quality/wpmvc-addon-testsuite": "dev-main"
    },
    "autoload": {
        "files": [
            "addon/Lib/functions.php"
        ],
        "psr-4": {
            "WPMVC\\Addons\\Customizer\\": "addon/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/framework/"
        ]
    },
    "minimum-stability": "dev"
}
